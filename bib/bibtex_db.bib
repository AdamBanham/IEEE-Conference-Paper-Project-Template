}

@InCollection{Cancela2020,
  author     = {Antonio Cancela and Antonia M. Reina Quintero and Mar{\'{\i}}a Teresa G{\'{o}}mez-L{\'{o}}pez and Alejandro Garc{\'{\i}}a-Garc{\'{\i}}a},
